package com.project14.nccu105.project14_2;

public class CardViewMember {
    private int id;
    private int image;
    private String name;

    public CardViewMember() {
        super();
    }

    public CardViewMember(int id, int image, String name) {
        super();
        this.id = id;
        this.image = image;
        this.name = name;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public int getImage() {
        return image;
    }

    public void setImage(int image) {
        this.image = image;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }
}
